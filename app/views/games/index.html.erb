<div class="breadcrumb breadcrumb2">当前位置：<a href="#">首页</a> > <span>获奖名单</span></div>
<!--hjmd1 begin-->
<div class="warpbox">
  <div class="bordertop">
    <div class="hjmd_box1">
      <div class="hjmd_1">
        <div class="hjmd_1_left"></div>
        <div class="hjmd_1_middle">
          <div class="hjmd_1_middle_top">
            <img src="/assets/images/hjmd/hjmd_top1.jpg" width="296" height="58" />
          </div>
          <div class="hjmd_1_middle_down">
            <div class="hjmd_1_middle_content">
              <div class="hjmd_1_middle_content_search">
              	<%= simple_form_for( "search",:url=>"",:html=>{ :method => :get},:remote=>true ) do |f| %>
              		<%= f.hidden_field :level,:value=>:beginner %>
	                <table width="250" border="0">
	                  <tr>
	                    <td width="65">
	                    	<%= f.select :province_id, @zones,:prompt => '请选择' %>
	                    </td>
	                    <td width="120">
	                    	<%= f.input_field :username, :style=>"width:114px; height:19px; color:#B6B6B6; line-height:19px; font-size:14px;" %>
	                    </td>
	                    <td width="51"><div class="hjmd_btn_search"><input name="" type="submit" value="搜索" /></div></td>
	                  </tr>
	                </table>
                 <% end %>
              </div>
              <% beginners = @awards_hash["beginner"] %>
              <span id = 'beginner_content'>
              	<%= render(:partial =>"award",:collection=>beginners) %>
              </span>
              <span id = 'beginner_paginate'>
              	<%= beginners and paginate beginners,:remote => true, :params => {:level => :beginner} %>
              </span>
            </div>
          </div>
        </div>
        <div class="hjmd_1_right"></div>
      </div>
      <div class="hjmd_1">
        <div class="hjmd_1_left"></div>
