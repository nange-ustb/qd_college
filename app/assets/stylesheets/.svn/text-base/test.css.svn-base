/*pagination*/
.pagination3{ height:30px; margin:20px;color:#676767;}
.pagination3 li{ float:left; width:auto;}
.pagination3 a{ border-radius:3px 3px 3px 3px; border: 1px solid #CCCCCC; margin:0 3px; float: left; font-family: arial; height: 20px; line-height: 20px; margin-left: 2px;overflow: hidden; padding: 1px 10px;}
.pagination3 li.first{ position:relative;}
.pagination3 li.first a{padding-left:15px;}
.pagination3 li.last{ position:relative;}
.pagination3 li.last a{ padding-right:15px;}
.pagination3 li.first b,.pagination li.last b{background-image:url(/assets/images/sm.png);background-repeat: no-repeat;height:12px; display:inline-block;width: 5px;overflow: hidden; position: absolute;}
.pagination3 li.first b{ background-position:-168px -23px; left:10px; top:6px;}
.pagination3 li.last b{background-position:-175px -23px; right:10px; top:6px;}

